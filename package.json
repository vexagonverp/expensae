{
  "name": "expensae",
  "productName": "expensae",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "app/dist/main/main.js",
  "scripts": {
    "build": "yarn clean:dist && cross-env NODE_ENV=production ELECTRON_ENV=production vite build --config configs/vite.config.ts",
    "build:debug": "yarn clean:build && yarn debug && electron-builder build",
    "ci": "yarn install --frozen-lockfile",
    "clean": "ts-node --esm configs/clean.ts --clean",
    "clean:build": "ts-node --esm configs/clean.ts --build",
    "clean:dist": "ts-node --esm configs/clean.ts --dist",
    "debug": "yarn clean:dist && cross-env NODE_ENV=production ELECTRON_ENV=debug vite build --config configs/vite.config.ts",
    "dev": "cross-env NODE_ENV=development ELECTRON_ENV=debug vite --config configs/vite.config.ts",
    "format": "prettier --write --cache .",
    "format:check": "prettier --check --cache .",
    "lint": "eslint --fix --cache .",
    "lint:check": "eslint --cache .",
    "make:linux": "yarn clean:build && yarn build && electron-builder build -l",
    "make:mac": "yarn clean:build && yarn build && electron-builder build -m",
    "make:win": "yarn clean:build && yarn build && electron-builder build -w",
    "prepare": "husky install",
    "prod": "yarn build && electron .",
    "prod:debug": "yarn debug && electron .",
    "start": "ts-node --esm configs/check-port.ts && yarn clean:dist && yarn dev",
    "upgrade:dependencies": "npx npm-check-updates -u -f /^\\(\\(?\\!\\(^chalk$\\|^@types\\/node$\\)\\).\\)*$/"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format:check",
      "yarn lint:check"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@reduxjs/toolkit": "^1.9.3",
    "antd": "^5.3.2",
    "electron": "^23.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-shared-state": "^1.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "node-schedule": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/detect-port": "^1.3.2",
    "@types/express": "^4.17.17",
    "@types/node": "^16.18.13",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "detect-port": "^1.5.1",
    "dotenv": "^16.0.3",
    "electron-builder": "^23.6.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.6",
    "sass": "^1.60.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "^4.0.7"
  },
  "engines": {
    "node": ">=16.x",
    "npm": ">=8.x"
  }
}
